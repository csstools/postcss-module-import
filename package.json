{
  "name": "postcss-module-import",
  "version": "1.0.0",
  "description": "Inline @import statements using CSS Modules",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugins",
    "sugar",
    "sass",
    "scss",
    "imports",
    "styles",
    "files",
    "partials",
    "modules",
    "bower",
    "node",
    "npm"
  ],
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": "jonathantneal/postcss-module-import",
  "bugs": "https://github.com/jonathantneal/postcss-module-import/issues",
  "homepage": "https://github.com/jonathantneal/postcss-module-import#readme",
  "dependencies": {
    "fs-extra": "^0.30.0",
    "fs-promise": "^0.5.0",
    "object-assign": "^4.1.0",
    "postcss": "^5.0.21",
    "resolve": "^1.1.7"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "eslint": "^2.13.1",
    "eslint-config-10up": "github:jonathantneal/eslint-config-10up",
    "jscs": "^3.0.5",
    "jscs-config-10up": "github:jonathantneal/jscs-config-10up",
    "normalize-css": "^2.3.1",
    "postcss-modules": "^0.5.0",
    "postcss-partial-import": "^2.0.0"
  },
  "scripts": {
    "lint": "eslint index.js && jscs index.js",
    "tape": "ava",
    "test": "npm run lint && npm run tape"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "eslintConfig": {
    "extends": "10up",
    "rules": {
      "consistent-return": [
        0
      ],
      "max-params": [
        2,
        4
      ],
      "no-param-reassign": [
        0
      ],
      "no-console": [
        0
      ]
    }
  },
  "jscsConfig": {
    "preset": "10up"
  }
}
